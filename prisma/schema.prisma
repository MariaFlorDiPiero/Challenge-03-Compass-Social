generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())

  username String @unique
  password String
  name String
  email String @unique
  birthdate String?
}

model Post {
  id Int @id @default(autoincrement())

  username String
  post_date String
  description String
  likes String
  url_image String?
}


